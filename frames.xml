<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">



	<Frame name="IconSelectorSectionTemplate" mixin="IconSelectorSectionMixin" virtual="true">
		
		<Size x="474" y="36"/>
		<Anchors>
			<Anchor point="TOPLEFT" x="20" y="0"/>
		</Anchors>
		<Frames>
			<Button parentKey="Title" >
				<Size x="474" y="36"/>
				<Anchors>
					<Anchor point="TOPLEFT" x="20" y="0"/>
				</Anchors>
				<ButtonText parentKey="ButtonText" nonspacewrap="true" justifyH="LEFT">
					<Size x="234" y="10"/>
					<Anchors>
						<Anchor point="LEFT" x="8" y="1"/>
					</Anchors>
				</ButtonText>
				<NormalTexture atlas="questlog-tab"/>
				<HighlightTexture atlas="questlog-tab" alphaMode="ADD" alpha="0.4"/>
				<NormalFont style="Game15Font_Shadow"/>
				<HighlightFont style="Game15Font_Shadow"/>
			</Button>

			<Frame parentKey="IconSelector" inherits="SelectorTemplate" mixin="IconSelectorMixin" >
				<KeyValues>
					<KeyValue key="buttonTemplate" value="IconSearchButtonTemplate" type="string"/>
				</KeyValues>
				<Size x="494" y="361"/>
				<Anchors>
					<Anchor point="TOPLEFT" x="21" y="-22"/>
				</Anchors>
			</Frame>
		</Frames>
		<Scripts>
			<OnLoad method="OnLoad"/>
		</Scripts>
	</Frame>

	<Button name="IconSearchButtonTemplate" mixin="IconSearchButtonMixin" inherits="SelectorButtonTemplate" virtual="true">
		<Layers>
			<Layer level="OVERLAY">
				<FontString parentKey="Name" inherits="GameFontHighlightSmallOutline">
					<Size x="36" y="10"/>
					<Anchors>
						<Anchor point="BOTTOM" x="0" y="2"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnEnter method="OnEnter"/>
			<OnLeave method="OnLeave"/>
		</Scripts>
	</Button>



	<Frame name="IconSearchFrame" parent="MacroPopupFrame" mixin="IconSearchMixin" >
		<Size x="494" y="361"/>
		<Anchors>
			<Anchor point="TOPLEFT" x="0" y="-75"/>
		</Anchors>
		<Frames>
			<EditBox name="IconSearchSearchBarTemplate" parentKey="SearchBar" autoFocus="false" historyLines="1" bytes="64" mixin="IconSearchSearchBarMixin" inherits="SearchBoxTemplate">
				<Size x="241" y="22"/>
				<Anchors>
					<Anchor point="TOPRIGHT" x="0" y="0" />
				</Anchors>
				<Scripts>
					<OnLoad method="OnLoad" />
					<OnEnterPressed method="OnEnterPressed" />
					<OnKeyUp method="OnKeyUp" />
				</Scripts>
			</EditBox>
			<Frame parentKey="NoSearchResultsText" hidden="true">
				<Layers>
					<Layer level="ARTWORK">
						<FontString inherits="SystemFont_Med3" text="QUEST_LOG_NO_RESULTS" >
							<Size x="250"/>
							<Anchors>
								<Anchor point="TOP"  relativeKey="$parent.MacroSelector" relativePoint="TOP" y="-30"/>
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
			</Frame>
			
			<ScrollFrame parentKey="IconSectionSelector"    mixin="IconSectionSelectorMixin" inherits="UIPanelScrollFrameCodeTemplate">
				<Size x="494" y="361"/>
				<Anchors>
					<Anchor point="TOPLEFT" x="0" y="-25"/>
					<Anchor point="BOTTOMRIGHT" x="0" y="0"/>
				</Anchors>
				<ScrollChild>
					<Frame parentKey="Contents">
						<Size x="494" y="0"/>
						<Anchors>
							<Anchor point="TOPLEFT" x="0" y="0"/>
							<Anchor point="BOTTOMRIGHT" x="0" y="0"/>
						</Anchors>
					</Frame>
				</ScrollChild>
				<Scripts>
					<OnLoad method="OnLoad"/>
				</Scripts>
			</ScrollFrame>


			<!-- <Frame parentKey="IconSelector" inherits="ScrollBoxSelectorTemplate" frameStrata="HIGH" hidden="true">
				<KeyValues>
					<KeyValue key="buttonTemplate" value="IconSearchButtonTemplate" type="string"/>
				</KeyValues>
				<Size x="494" y="361"/>
				<Anchors>
					<Anchor point="TOPLEFT" x="21" y="-22"/>
				</Anchors>
			</Frame> -->
		</Frames>
		<Scripts>
			<OnLoad method="OnLoad"/>
		</Scripts>
	</Frame>




	

</Ui>